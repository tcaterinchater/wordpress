		<script type="text/javascript">
		// <![CDATA[
			if(! window.com) { window.com = {}; }
			if(! window.com.cnn) { window.com.cnn = {}; }
			if(! window.com.cnn.blogs) { window.com.cnn.blogs = {}; }
			if(! window.com.cnn.blogs.belief) { window.com.cnn.blogs.belief = {}; }
			if(! window.com.cnn.blogs.belief.featuredPosts) { window.com.cnn.blogs.belief.featuredPosts = {}; }
			if(! window.com.cnn.blogs.belief.featuredPosts.util) { window.com.cnn.blogs.belief.featuredPosts.util = {}; }

			com.cnn.blogs.belief.featuredPosts.util.transformList = function(args) {
				var settings = Object.extend({
					galleryContainerId : "cnn_GallerySliderContainer",
					galleryStripClass : "cnn_fabcaslab",
					galleryCatClass : "cnn_fabcatz",
					size : 4
				}, args);

				var grps = $$("#" + settings.galleryContainerId + " ul li").inGroupsOf(settings.size);
				var slides = grps.inject([], function(slds, v, i) {
					var li = v.inject([], function(lis, v, i) {
						var link, img, title;

						if(v === null) { return lis; }

						// TRANSFORM ENTRY
						link = v.down().href;
						img = v.down('img').remove();
						title = "<a href='" + link + "'>" + v.down().innerHTML + "</" + "a>";

						img = img.wrap('a', { 'href' : link });
						img = img.wrap('blockquote');
						v.update();
						v.insert(img);
						v.insert(title);
						// TRANSFORM ENTRY

						lis += "<div class='"+ settings.galleryCatClass + "'>" + v.innerHTML  + "</" + "div>";

						return lis;
					});

					slds += "<div class='" + settings.galleryStripClass + "'>" + li + "</" + "div>";
					return slds;
				});

				$(settings.galleryContainerId).update(slides);
			};

			com.cnn.blogs.belief.randomBanner = function() {
				var randBanner = "banner" + (Math.floor(Math.random()*4) + 1);

				if(null !== $("cnn-banner")) {
					$("cnn-banner").addClassName(randBanner);
				}

				if(null !== $$("#cnnContentContainer .cnnGradWrap")) {
					Element.addClassName($$("#cnnContentContainer .cnnGradWrap")[0], randBanner);
				}
			}

			document.observe("dom:loaded", function() {

				com.cnn.blogs.belief.randomBanner();

				if ( $$('#cnn-ireport-viewer #cnn-ireport-container').length > 0 && typeof com.cnn.blogs.featuredPosts.ireport !== "object") {

					com.cnn.blogs.featuredPosts.ireport = new com.cnn.blogs.featuredPosts.FeaturedPosts({
						'galleryContainerId' : 'cnn-ireport-container',
						'galleryStripClass' : 'cnn-ireport-slab',
						'nextButtonContainerId' : 'cnn-ireport-next',
						'prevButtonContainerId' : 'cnn-ireport-prev',
						'displayWidth' : 300					});
					Effect.Appear('cnn-ireport-viewer' );
				}
			});
		// ]]>
		</script>
