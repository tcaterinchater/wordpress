<script>

function player(play,w,h,name) {   
    var play ;        
    var name ;
	if ( h < ( screen.availHeight - 50 ) ) {
		h = h; }
	else {
		h = ( screen.availHeight - 50 ); }
    viewer = "width="+w+",height="+h+",toolbar=no,scrollbars=yes,resizable=yes,status=no,location=no,directories=no";
    popWin = window.open(play, name, viewer);
}

</script>

<script type="text/javascript">
var originalTitle = "Bill O'Reilly: Video Center";
var titleWithCat = "";

var selected = '0';
//var tabState = '';

function loadVideo(vid, tabid, isLatestVideo) {
	new Ajax.Updater('vidspace', '/pg/jsp/video/selectedvideo.jsp?chartID='+tabid, {parameters:'vid=' + vid, evalScripts:true, asynchronous:true, method:'post'});
	new Ajax.Updater('descspace', '/pg/jsp/video/selecteddesc.jsp?chartID='+tabid, {parameters:'vid='+vid+'&latestVideos='+isLatestVideo, evalScripts:true, asynchronous:true, method:'post'});		
	setSelected(tabid, vid);
}

function loadFoxVideo(miniID, tabid, isLatestVideo) {
		new Ajax.Updater('vidspace', '/agnosticchart', {parameters:'charttype=minichart&chartID='+tabid+'&formatID=1&size=999&searchColumn=uniqueID&searchText='+miniID+'&useMiniChartID=true&destinationpage=/pg/jsp/video/foxvideosajax.jsp', evalScripts:true, asynchronous:true, method:'post'});
		new Ajax.Updater('descspace', '/agnosticchart', {parameters:'charttype=minichart&chartID='+tabid+'&latestVideos='+isLatestVideo+'&formatID=1&size=999&searchColumn=uniqueID&searchText='+miniID+'&useMiniChartID=true&destinationpage=/pg/jsp/video/selectedfox.jsp', evalScripts:true, asynchronous:true, method:'post'});		
		setSelected(tabid, miniID);
}
	
function loadMinichartVideo(vid, tabid, isLatestVideo){
	new Ajax.Updater('vidspace', '/agnosticchart', {parameters:'charttype=minichart&chartID='+tabid+'&formatID=1&size=999&vid='+vid+'&useMiniChartID=true&destinationpage=/pg/jsp/general/videosinclude.jsp', evalScripts:true, asynchronous:true, method:'post'});
	new Ajax.Updater('descspace', '/agnosticchart', {parameters:'charttype=minichart&chartID='+tabid+'&formatID=1&size=999&vid='+vid+'&latestVideos='+isLatestVideo+'&useMiniChartID=true&destinationpage=/pg/jsp/video/selectedinclude.jsp', evalScripts:true, asynchronous:true, method:'post'});	
	setSelected(tabid, vid);
}	

function loadYouTubeVideo(vid, tabid, isLatestVideo){
	new Ajax.Updater('vidspace', '/agnosticchart', {parameters:'charttype=minichart&chartID='+tabid+'&formatID=1&size=999&vid='+vid+'&useMiniChartID=true&destinationpage=/pg/jsp/video/youtubeinclude.jsp', evalScripts:true, asynchronous:true, method:'post'});
	new Ajax.Updater('descspace', '/agnosticchart', {parameters:'charttype=minichart&chartID='+tabid+'&latestVideos='+isLatestVideo+'&formatID=1&size=999&vid='+vid+'&useMiniChartID=true&destinationpage=/pg/jsp/video/selectedyoutube.jsp', evalScripts:true, asynchronous:true, method:'post'});	
	setSelected(tabid, vid);
}	
	
	
	
	function setSelected(which, id ) {

		if(selected != "0" && document.getElementById(selected) != null){
		document.getElementById(selected).style.backgroundColor = "#ffffff";
		}
		if(id != "0" && document.getElementById(id) != null){
		document.getElementById(id).style.backgroundColor = "#DEDFEF";	
		}
		
		selected = id;	
		
	}
	
var currentActiveYear = 2013;
var currentActiveMonth = 10;
var currentActiveLoadYear = 2013;
function selectVidCalYear(year, changeYear) {
		for (var p=2004;p<2013+1; p++){
			$('videoArchiveYear' + p).className = 'torfArchiveCalYearUnselected';
		}
		$('videoArchiveYear' + year).className = 'torfArchiveCalYearSelected';
		if(changeYear !=null){
		currentActiveYear = year;
		}
}
function selectVidCalMonth(month, year) {
	selectVidCalYear(year, true);
	//if (month != currentActiveMonth || year != currentActiveLoadYear) {
		$('videoArchiveMonth' + currentActiveLoadYear + '-' + currentActiveMonth).className = 'torfArchiveCalMonthUnselected';
		$('videoArchiveMonth' + year + '-' + month).className = 'torfArchiveCalMonthSelected';
		currentActiveLoadYear = year;
		currentActiveMonth = month;
	//}
}

function ajaxUpdate(container, path, start) {
	new Ajax.Updater(container, path + '&start=' + start + '&vid='+selected);
}
function showMouseover(){
	ddrivetip("You must log in as a Premium Member to view.","#4c4c4c", 125);
}

function changeFilter(checkboxClicked){
	if(checkboxClicked != null){
		
		var clickedElement = jQuery(checkboxClicked);
		if(clickedElement.attr("class").indexOf("selectedCategoryToggle") != -1){
			var associatedShows = clickedElement.data("associatedShows");									
			jQuery(".selectedVideoFilter").each(function(index, element){
				var checkbox=jQuery(element);
				var checkboxValue = checkbox.val();
				if(associatedShows.indexOf(checkboxValue) != -1){
					console.log("happened");	
					if(clickedElement.prop('checked') == true){
						checkbox.prop('checked', true);
					} else {
						checkbox.prop('checked', false);
					}
				} 
			});
		}
	}
	var oFilterChecked = true;
	jQuery('.selectedVideoFilter.oFactor').each(function(index,element){
		if(jQuery(element).prop("checked") == false){
			oFilterChecked = false;
		}
	});
	if(oFilterChecked){
		jQuery('.selectedCategoryToggle.oFactor').prop("checked", true);
	} else {
		jQuery('.selectedCategoryToggle.oFactor').prop("checked", false);
	}
	var factorOnlineChecked = true;
	jQuery('.selectedVideoFilter.factorOnline').each(function(index,element){
		if(jQuery(element).prop("checked") == false){
			factorOnlineChecked = false;
		}
	});
	if(factorOnlineChecked){
		jQuery('.selectedCategoryToggle.factorOnline').prop("checked", true);
	} else {
		jQuery('.selectedCategoryToggle.factorOnline').prop("checked", false);
	}
	filterVideosToDisplay();
}
</script>
