                <td valign="top" style="padding-left:14px;">
                	<script type="text/javascript">
					var sortByParam = "desc";
					var amtToDisplayParam = 12;
					var endDate = "2013-11-08";
					var beginDate = "2004-01-01";
					function filterVideosToDisplay(begin, end, sortBy, amountToDisplay){
						if((sortBy != null && sortBy != sortByParam) || sortBy == null){
							
							if(sortBy != null){
							$("newest").className = "torfArchiveSortUnselected";
							$("oldest").className = "torfArchiveSortUnselected";
							if(sortBy == "asc"){
								$("oldest").className = "torfArchiveSortSelected";
							}else {
								$("newest").className = "torfArchiveSortSelected";
							}
							sortByParam=sortBy;
							}
						if(amountToDisplay != null){
							$("12perPage").className = "torfArchiveSortUnselected";
							$("36perPage").className = "torfArchiveSortUnselected";
							if(amountToDisplay == "12"){
								$("12perPage").className = "torfArchiveSortSelected";
							}else {
								$("36perPage").className = "torfArchiveSortSelected";
							}
							amtToDisplayParam = amountToDisplay;
						}
						if (begin != null){
							beginDate=begin;
							
						}
						if (end != null){
							endDate=end;
						}
						var isLatestVideos = false;
						var selectedCats = jQuery(".selectedVideoFilter");
						var selectedCatsParam = "";
						for(j=0; j<selectedCats.size(); j++){
							if (selectedCats[j].checked){
								selectedCatsParam += selectedCats[j].value + ";";
							}
						}
						if(selectedCatsParam.length == 0){
							isLatestVideos = true;
							selectedCatsParam = "303;307;310;315;321;322;554;556;558;610;678;323;";
						}
						new Ajax.Updater('filteredVideoContainer', '/pg/jsp/video/filterajaxvideoinclude.jsp', {parameters:'cats=' + selectedCatsParam + "&sortBy=" + sortByParam + "&amountToDisplay=" + amtToDisplayParam + "&end="+endDate+"&begin="+beginDate+"&latestVideos="+isLatestVideos, evalScripts:true, asynchronous:true, method:'post'});
						}
					}
					function selectUnselectAll(which){
						var allCats = jQuery(".selectedVideoFilter");
						if(which != null){
						  if(which=="select"){
							  jQuery('.selectedCategoryToggle').prop('checked', true);
							  for(j=0; j<allCats.size(); j++){
								  if (!allCats[j].checked){
									  allCats[j].checked = true;
								  }
							  }
						  } 
						  if (which=="unselect") {
							  jQuery('.selectedCategoryToggle').prop('checked', false);
							  for(j=0; j<allCats.size(); j++){
								  if (allCats[j].checked){
									  allCats[j].checked = false;
								  }
							  }
						  }
						}
						filterVideosToDisplay();
					}
					
					</script>
